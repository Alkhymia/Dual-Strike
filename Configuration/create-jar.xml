<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project Dual Strike Configuration">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <delete file="mcc.jar"/>
        <jar destfile="mcc.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="dualstrike.configuration.Configuration"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="java" excludes="**/*.java"/>
        </jar>
    </target>
</project>
